
<div class="detail">
  <img src="<%= pokemon.escape('image_url') %>" ><br>
  <p>
    Name: <%= pokemon.escape('name') %>
  </p>

  <% for (var attr in pokemon.attributes) { %>
    <% if(attr != 'id' && attr != 'image_url' && attr != 'name') { %>
      <p>
        <%= attr + ": " + pokemon.escape(attr) %>
      </p>
    <% } %>
  <% } %>

</div>
<h5>Toys: </h5>
<ul class="toys">

</ul>
